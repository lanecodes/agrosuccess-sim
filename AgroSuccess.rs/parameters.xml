<?xml version="1.0" encoding="UTF-8" ?>
<parameters>

	<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
			defaultValue="__NULL__" 
	/>
	
	<!-- 
	Path to study site data inputs for the model. It is assumed that this 
	directory will contain a subdirectory with the name of each study site, 
	with spaces represented with underscores, and any unicode characters 
	replaced with a representative ASCII alternative. 
	 -->	
	<parameter 
		name="geoDataDirRootString" 
		displayName="Geographic Data Root Directory" 
		type="java.lang.String" 
		defaultValue="data/geographic" 
		isReadOnly="true" 
	/>
	
	<!-- 
	Name of study site used for model run. This parameter is read/write to 
	allow a model sweep to load data for different study sites as needed.
	For simple cases however, one can simply set a default value below. 
	 -->
	<parameter 
	 	name="studySite" 
		displayName="Study site for simulation" 
		type="java.lang.String" 
		defaultValue="navarres" 
		isReadOnly="true" 
	/>	
	
</parameters>
